# sicsim> history 
# sicsim> hi 

//type filename
#sicsim> type [filename]

//memory command
//dump memory address and value
# sicsim> dump [start, end] 
# sicsim> du [start, end] 

//edit memory 
# sicsim> edit address, value 
# sicsim> e address, value 

//fill memory with value
# sicsim> fill start, end, value 
# sicsim> f start, end, value
 
//reset whole memory to 0x00
# sicsim> reset 



//opcode command
//find menmonic's opcode
# sicsim> opcode mnemonic

//print optable  
# sicsim> opcodelist 



//assembler
//make listing file and object file
# sicsim> assemble [filename]



//linkingloader command
//set program address
# sicsim> progaddr [address]

//load object files
# sicsim> loader [objectfile] [objectfile] [...]

//add break point
# sicsim> bp [address]

//clear all breakpoints
# sicsim> clear bp

//print all breakpoints
# sicsim> bp

//execute program considering breakpoints
# sicsim> run


